{
  "metadata": [
    {
      "properties": {
          "TargetFramework": "net451"
      },
      "src": [
        {
          "files": [
            "CK.Text/CK.Text.csproj",
            "CK.Text.Virtual/CK.Text.Virtual.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "doc/**"
          ]
        }
      ],
      "dest": "doc/api"
    }
  ],
  "build": {
    "properties": {
        "TargetFramework": "net451"
    },
    "content": [
      {
        "src": "doc/",
        "files": [
          "api/toc.yml",
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "doc/",
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "doc/",
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "packages/msdn.4.5.2.0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"
    ],
    "dest": "doc/_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "CK-Text",
      "_appName": "CK-Text",
      "_enableSearch": true,
      "_appFooter": "<span>Copyright Â© 2015-2017 Invenietis<br>Generated by <strong><a href=\"https://dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a></strong></span>"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "doc/template"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false
  }
}