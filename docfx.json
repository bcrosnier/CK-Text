{
  "metadata": [
    {
      "properties": {
          "TargetFramework": "net451"
      },
      "src": [
        {
          "files": [
            "CK.Text/CK.Text.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "doc/**"
          ]
        }
      ],
      "dest": "doc/api"
    }
  ],
  "build": {
    "properties": {
        "TargetFramework": "net451"
    },
    "content": [
      {
        "files": [
          "doc/api/toc.yml",
          "doc/api/**.yml",
          "doc/api/index.md",
          "doc/articles/**.md",
          "doc/articles/**/toc.yml",
          "doc/toc.yml",
          "doc/*.md"
        ],
        "exclude": [
          "obj/**",
          "doc/_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "doc/images/**"
        ],
        "exclude": [
          "obj/**",
          "doc/_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "doc/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "doc/_site/**"
        ]
      }
    ],
    "xref": [
      "packages/msdn.4.5.2.0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"
    ],
    "dest": "doc/_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "CK-Text",
      "_enableSearch": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false
  }
}